; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Android Plugin Demo"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/viewport_width=648
window/size/viewport_height=1152
window/size/transparent=true
window/per_pixel_transparency/allowed=true
window/handheld/orientation=1

[dotnet]

project/assembly_name="Android Plugin Demo"

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotAndroidPluginTemplate/plugin.cfg")

[rendering]
viewport.transparent_background = true
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
viewport/transparent_background=true
environment/default_clear_color=Color(0, 0, 0, 0)
